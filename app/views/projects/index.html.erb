<div class="overall-cont">
  <%= render "shared/lhs_nav" %>
  <div class="rhs-cont">
    <p id="notice"><%= notice %></p>
    <div class="container-fluid">
      <div class="row">
        <% @projects.each do |project| %>
          <div class="col-4">
            <%= link_to project_path(project) do %>
              <div class="card">
                <div class="card-title">
                  <%= project.name %>
                </div>
              </div>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
