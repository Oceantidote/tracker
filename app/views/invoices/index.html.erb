<div class="overall-cont">
  <div class="rhs-cont">
    <div class="container-fluid">
      <table>
        <thead>
          <tr>
            <th>User</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @invoices.each do |invoice| %>
            <tr>
              <td><%= invoice.user_id %></td>
              <td><%= link_to 'Show', invoice %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
